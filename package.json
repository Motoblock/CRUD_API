{
  "name": "crud_api",
  "version": "1.0.0",
  "description": "CRUD API using in-memory database underneath",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "start:dev": "nodemon --exec ts-node-esm src/index.ts",
    "start:prod": "webpack && node dist/index.cjs",
    "start:multi:dev": "nodemon --exec ts-node-esm ./src/index.ts -- --multi",
    "start:multi:prod": "webpack && node dist/index.cjs -- --multi",
    "lint": "eslint . --ext .tsx --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Motoblock/CRUD_API.git"
  },
  "keywords": [],
  "author": "Motoblock",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Motoblock/CRUD_API/issues"
  },
  "homepage": "https://github.com/Motoblock/CRUD_API#readme",
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@types/uuid": "^9.0.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.4",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  }
}